<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Assignment2.Entities.AdministratorsEntity" table="Administrators" schema="dbo" catalog="ronigu">
        <id name="adminid">
            <column name="ADMINID" sql-type="numeric(18)" precision="18"/>
        </id>
        <property name="username">
            <column name="USERNAME" sql-type="nvarchar(150)" length="150" not-null="true"/>
        </property>
        <property name="password">
            <column name="PASSWORD" sql-type="nvarchar(150)" length="150" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>