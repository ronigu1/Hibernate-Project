<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Entities.LoginLogEntity" table="LoginLog" schema="dbo" catalog="ronigu">
        <composite-id mapped="true" class="Entities.LoginLogEntityPK">
            <key-property name="userid">
                <column name="USERID" sql-type="numeric(18)" precision="18"/>
            </key-property>
            <key-property name="logintime">
                <column name="LOGINTIME" sql-type="datetime"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>